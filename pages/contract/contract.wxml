		</view>
<button  class="weui-btn weui-btn_primary" bindtap='getImg'>
  选择图片
</button>
		

<view class="imageSizi">
<image src='{{imgpath1}}' ></image>
</view>

<button class="weui-btn weui-btn_primary" bindtap='uploadImg'> 
  确定上传
</button>

	<view wx:if="{{updataImage1Flog}}">
	<view class="weui-progress">
			<view class="weui-progress__bar">
				<view class="weui-progress__inner-bar js_progress" style="width: {{updataImage1}}%;"></view>
			</view>
			<a class="weui-progress__opr">
				<i class="weui-icon-cancel"></i>
